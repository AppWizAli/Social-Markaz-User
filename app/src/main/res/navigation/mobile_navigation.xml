<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
       android:name="com.socialmarkaz.app.Fragments.FragmentHome"
        android:label="Home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_cart"
android:name="com.socialmarkaz.app.Fragments.FragmentCart"
        android:label="Cart"
        tools:layout="@layout/fragment_cart" />

    <fragment
        android:id="@+id/navigation_myaccount"
    android:name="com.socialmarkaz.app.Fragments.FragmentMyAccount"
    android:label="account"
        tools:layout="@layout/fragment_my_account" />
    <fragment
        android:id="@+id/navigation_search"
  android:name="com.socialmarkaz.app.Fragments.FragmentSearch"
        android:label="search"
        tools:layout="@layout/fragment_search" />
</navigation>